@{
    ViewBag.Title = "Renew | Felanmälan";
    Layout        = "~/Views/Shared/_Layout.cshtml";
}

@section scripts
{
    @Scripts.Render("~/site-bugreport/js")
}

<!-- Page layout -->

<div class="container">
    <div class="row">
        <div class="col-md-3 site-page-control">
            <h3 class="site-page-header">Felanmälan</h3>
            <div class="row">
                <div class="col-md-12">
                    <h4>Ny felanmälan</h4>
                    <p>Här kan du skapa nya felanmälningar och skicka direkt till vårt system för ärendehantering.</p>
                    <p>Det går också att nå vår support via andra kanaler:</p>
                    <p>Itancan Consulting AB<br />www.itancan.com<br />support@itancan.com</p>
                </div>
            </div>
        </div>
        <div class="col-md-9 site-page-content">
            <div class="row" id="create-bugreport-panel">
                <div class="col-md-12">
                    @Html.Partial("BugReportCreate")
                </div>
            </div>
        </div>
    </div>
</div>
